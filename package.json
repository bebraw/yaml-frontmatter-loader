{
  "name": "yaml-frontmatter-loader",
  "version": "0.1.0",
  "description": "YAML frontmatter loader for webpack",
  "license": "MIT",
  "repository": "webpack-contrib/yaml-frontmatter-loader",
  "author": "eldh",
  "homepage": "https://github.com/webpack-contrib/yaml-frontmatter-loader",
  "bugs": "https://github.com/webpack-contrib/yaml-frontmatter-loader/issues",
  "main": "src/index.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "scripts": {
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --cache src test",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "release": "standard-version",
    "release:ci": "conventional-github-releaser -p angular",
    "release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
    "security": "nsp check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "ci:lint": "npm run lint && npm run security",
    "ci:test": "npm run test -- --runInBand",
    "ci:coverage": "npm run test:coverage -- --runInBand",
    "defaults": "webpack-defaults"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "webpack",
    "yaml",
    "frontmatter"
  ],
  "peerDependencies": {
    "webpack": "^4.3.0"
  },
  "dependencies": {
    "front-matter": "^2.1.2",
    "loader-utils": "^1.1.0",
    "schema-utils": "^0.4.5"
  },
  "devDependencies": {
    "webpack-defaults": "^2.1.1"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
